{
	"name" : "PPP Tools",
	"version" : "2.0",
	"manifest_version": 3,
	"background": {
    "service_worker": "background.js"
	},
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["popup-form.js"]
  }],
	"icons": { "128": "images/logo.png" },
	"action": {
		"default_icon" : "images/logo.png",
		"default_title" : "PPP工具库",
		"default_popup": "popup.html"
	},
  "permissions" : [ "tabs","notifications", "cookies", "storage"],
  "host_permissions": ["*://*/*", "<all_urls>"]
}